{
   "languageserver": {
      "clangd": {
         "command": "/usr/local/opt/llvm/bin/clangd",
         "args": [
            "--background-index"
         ],
         "rootPatterns": [
            "compile_flags.txt",
            "compile_commands.json",
            ".vim/",
            ".git/",
            ".hg/"
         ],
         "filetypes": [
            "c",
            "cpp",
            "objc",
            "objcpp"
         ]
      },
      //"metals": {
      //    "command": "metals-vim",
      //    "rootPatterns": ["build.sbt"],
      //    "filetypes": ["scala", "sbt"]
      //},
      "flow": {
         "command": "/usr/local/bin/flow",
         "args": [
            "lsp"
         ],
         "filetypes": [
            "javascript",
            "javascriptreact"
         ],
         "initializationOptions": {},
         "requireRootPattern": true,
         "rootPatterns": [
            ".flowconfig"
         ]
      }
   },
   "java.home": "/Library/Java/JavaVirtualMachines/openjdk-13.0.0.jdk/Contents/Home",
   "coc.preferences.formatOnType": true,
   "coc.preferences.formatOnSaveFiletypes": [
      "javascript",
      "js",
      "html",
      "Markdown",
      "css",
      "scala"
   ],
   "eslint.enable": true,
   "eslint.run": "onType",
   "eslint.autoFixOnSave": true,
   "eslint.filetypes": [
      "javascript",
      "typescript",
      "typescriptreact",
      "javascriptreact",
      "typescript.tsx",
      "vue"
   ],
   "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
   "cSpell.userWords": [
    "Avaito",
    "Interop",
    "Timeframe",
    "VCAP",
    "aircrafts",
    "autofix",
    "aviato",
    "gruvbox",
    "multiline",
    "nestjs",
    "postgres",
    "postgresql",
    "readonly",
    "timestamptz",
    "tpope",
    "typeorm",
    "uuidv",
    "varchar"
  ],
     "vetur.completion.useScaffoldSnippets": false,
    "vetur.experimental.templateInterpolationService": true,
    "vetur.format.defaultFormatter.ts":"vscode-typescript",  
    "codeLens.enable": true,
}
