{
   "languageserver": {
      "clangd": {
         "command": "/usr/local/opt/llvm/bin/clangd",
         "args": [
            "--background-index"
         ],
         "rootPatterns": [
            "compile_flags.txt",
            "compile_commands.json",
            ".vim/",
            ".git/",
            ".hg/"
         ],
         "filetypes": [
            "c",
            "cpp",
            "objc",
            "objcpp"
         ]
      },
      //"metals": {
      //    "command": "metals-vim",
      //    "rootPatterns": ["build.sbt"],
      //    "filetypes": ["scala", "sbt"]
      //},
      "flow": {
         "command": "/usr/local/bin/flow",
         "args": [
            "lsp"
         ],
         "filetypes": [
            "javascript",
            "javascriptreact"
         ],
         "initializationOptions": {},
         "requireRootPattern": true,
         "rootPatterns": [
            ".flowconfig"
         ]
      }
   },
   "java.home": "/Library/Java/JavaVirtualMachines/openjdk-13.0.0.jdk/Contents/Home",
   "coc.preferences.formatOnType": true,
   "coc.preferences.formatOnSaveFiletypes": [
      "javascript",
      "js",
      "html",
      "Markdown",
      "css",
      "scala"
   ],
   "eslint.enable": true,
   "eslint.run": "onType",
   "eslint.autoFixOnSave": true,
   "eslint.filetypes": [
      "javascript",
      "typescript",
      "typescriptreact",
      "javascriptreact",
      "typescript.tsx",
      "vue"
   ],
   "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
   "cSpell.userWords": [
    "Avaito",
    "Coplan",
    "DGRAM",
    "INET",
    "IPPROTO",
    "Interop",
    "Referer",
    "Timeframe",
    "UNSPEC",
    "VCAP",
    "Vuex",
    "aircrafts",
    "autofix",
    "aviato",
    "blns",
    "camelcase",
    "firenvim",
    "gruvbox",
    "ipaddr",
    "ipaddress",
    "krid",
    "mcoplan",
    "multiline",
    "nestjs",
    "postgres",
    "postgresql",
    "readonly",
    "timestamptz",
    "tpope",
    "trippin",
    "typeorm",
    "uuidv",
    "varchar",
    "vuetify",
    "whoami",
    "wordlist"
  ],
     "vetur.completion.useScaffoldSnippets": false,
    "vetur.experimental.templateInterpolationService": true,
    "vetur.format.defaultFormatter.ts":"vscode-typescript",
    "git.addGBlameToVirtualText": true, 
    "git.gitlab.hosts": ["https://git.aoc-pathfinder.cloud"],
    //"git.addedSign.text": "ðŸ’š" ,
    //"git.removedSign.text": "ðŸŸ¥" ,
    //"git.changedSign.text": "ðŸ”¹" ,
    //"git.changeRemovedSign.text": "ðŸ’¥" ,
    "codeLens.enable": true,
    "tsserver.execArgv": ["--nolazy", "--inspect-brk"],
    "tsserver.trace.server": "verbose",
     
     
}
