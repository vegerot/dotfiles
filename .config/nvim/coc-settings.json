{
  "languageserver": {
    "clangd": {
      "command": "/usr/lib/llvm-9/bin/clangd",
      "args": [
        "--background-index"
      ],
      "rootPatterns": [
        "compile_flags.txt",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "c",
        "cpp",
        "objc",
        "objcpp"
      ]
    },
    "metals": {
      "command": "metals-vim",
      "rootPatterns": [
        "build.sbt"
      ],
      "filetypes": [
        "scala",
        "sbt"
      ]
    },
    "python": {
      "command": "python",
      "args": [
        "-mpyls",
        "-vv",
        "--log-file",
        "/tmp/lsp_python.log"
      ],
      "filetypes": [
        "python"
      ],
      "settings": {
        "pyls": {
          "enable": true,
          "commandPath": "",
          "configurationSources": [
            "pycodestyle"
          ],
          "plugins": {
            "pyls_mypy": {
              "enabled": true,
              "live_mode": true
            }
          }
        }
      }
    }
  },
  "java.home": "/Library/Java/JavaVirtualMachines/openjdk-13.0.0.jdk/Contents/Home",
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "js",
    "html",
    "Markdown",
    "css",
    "scala"
  ],
  "yank.highlight.duration": 69,
  "eslint.enable": true,
  "eslint.run": "onType",
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
    "vue"
  ],
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "cSpell.userWords": [
    "Avaito",
    "Coplan",
